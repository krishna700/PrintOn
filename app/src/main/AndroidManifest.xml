<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.printon.user">

    <application
        android:name=".Util.App"
        android:allowBackup="true"
        android:icon="@mipmap/launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SubCategoriesActivity"></activity>
        <activity android:name=".Activities.AddDetailsActivity"></activity>
        <activity android:name=".Activities.MainActivity" />
        <activity
            android:name=".Activities.LoginActivity"
            android:noHistory="true" />
        <activity
            android:name=".Activities.SplashActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>